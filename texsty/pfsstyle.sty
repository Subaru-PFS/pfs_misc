\newcommand{\col}{
  \@afterindenttrue
  \secdef\@col\@scol
}
\def\@col[#1]#2{
  \set@colname{#2}
  \@makecolhead{#2}
  \@afterheading
}
\def\@makecolhead#1{
  \hbox{}
  {
    \parindent\z@
    \raggedright
    \setbox0=\hbox{\reset@font\Large\bfseries #1}
    \dimen0=\wd0
    \ifdim \dimen0 > \linewidth  \relax
      \vbox{
        \hbox to \linewidth{
          \reset@font\Large\bfseries #1\hfil
        }
      }
    \else
      \vbox{
        \hbox to \linewidth{
          \reset@font\Large\bfseries #1\hfil
        }
      }
    \fi
  }
  \nopagebreak
}
\def\@scol#1{
  \set@colname{#1}
  \@makecolhead{#1}
  \@afterheading
}


\newcommand{\subcol}{
  \@afterindenttrue
  \secdef\@subcol\@ssubcol
}
\def\@subcol[#1]#2{
  \set@subcolname{#2}
  \@makesubcolhead{#2}
  \@afterheading
}
\def\@makesubcolhead#1{
  \hbox{}
  {
    \parindent\z@
    \raggedright
    \setbox0=\hbox{\reset@font\large\bfseries #1}
    \dimen0=\wd0
    \ifdim \dimen0 > \linewidth  \relax
      \vbox{
        \hbox to \linewidth{
          \reset@font\large\bfseries #1\hfil
        }
      }
    \else
      \vbox{
        \hbox to \linewidth{
          \reset@font\large\bfseries #1\hfil
        }
      }
    \fi
  }
  \nopagebreak
}
\def\@ssubcol#1{
  \set@subcolname{#1}
  \@makesubcolhead{#1}
  \@afterheading
}


